{
  "name": "rmrk-listener",
  "version": "0.0.1",
  "description": "A Template on which to Build RMRK2 WebApps",
  "main": "src/index.js",
  "scripts": {
    "start-db": "docker-compose up postgres",
    "create-db": "npx ts-node src/bin/createdb.ts",
    "create-dump": "npx ts-node src/bin/create_tracked_collections_dump.ts",
    "consolidated2db": "npx ts-node src/bin/consolidated_dump_2_db.ts",
    "unconsolidated2db": "npx ts-node src/bin/unconsolidated_dump_2_db.ts",
    "start": "npx ts-node --transpile-only ./src/index.ts",
    "fetch": "rmrk-tools-fetch",
    "consolidate": "rmrk-tools-consolidate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express-rate-limit": "^6.4.0",
    "@polkadot/api": "^7.8.1",
    "@polkadot/keyring": "^8.4.1",
    "@types/node": "^17.0.18",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "JSONStream": "^1.3.5",
    "nanoid": "^3.1.25",
    "node-fetch": "^2.6.7",
    "node-fetch-progress": "^1.0.2",
    "object-hash": "^2.2.0",
    "p-limit": "^3.1.0",
    "pg": "^8.7.1",
    "pg-listen": "^1.7.0",
    "prettier": "^2.3.2",
    "ramda": "^0.27.1",
    "rmrk-tools": "^2.0.51",
    "shelljs": "^0.8.5",
    "socket.io": "^4.4.0",
